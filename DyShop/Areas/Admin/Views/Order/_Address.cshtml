@model DyShop.Areas.Admin.Models.OrderViewModel.AddressViewModel

<div class="form-group">
    <label asp-for="Name"></label>
    <input class="form-control" type="text" asp-for="Name">
    <span class="text-danger" asp-validation-for="Name"></span>
</div>
<div class="form-group">
    <label asp-for="Email"></label>
    <input class="form-control" type="text" asp-for="Email">
    <span class="text-danger" asp-validation-for="Email"></span>
</div>
<div class="form-group">
    <label asp-for="Phone"></label>
    <input class="form-control" type="text" asp-for="Phone">
    <span class="text-danger" asp-validation-for="Phone"></span>
</div>
<div class="form-group">
    <label asp-for="Street"></label>
    <input class="form-control" type="text" asp-for="Street">
    <span class="text-danger" asp-validation-for="Street"></span>
</div>
<div class="form-group">
    <label asp-for="City"></label>
    <input class="form-control" type="text" asp-for="City">
    <span class="text-danger" asp-validation-for="City"></span>
</div>
<div class="form-group">
    <label asp-for="ZipCode"></label>
    <input class="form-control" type="text" asp-for="ZipCode">
    <span class="text-danger" asp-validation-for="ZipCode"></span>
</div>
<div class="form-group">
    <label asp-for="Note"></label>
    <textarea class="form-control" asp-for="Note"></textarea>
    <span class="text-danger" asp-validation-for="Note"></span>
</div>

